<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/firstInform"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_centerVertical="true"
        android:paddingLeft="5dp">

        <TextView
            android:id="@+id/firstInformFirstTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:includeFontPadding="false"
            android:text="13:40"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/firstInformSecondTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/firstInformFirstTime"
            android:layout_alignParentLeft="true"
            android:includeFontPadding="false"
            android:text="15:20"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/firstInformPlace"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/firstInformSecondTime"

            android:layout_alignParentBottom="true"
            android:gravity="bottom|left"
            android:maxEms="5"
            android:singleLine="false"
            android:text="educa.isu.ru, meet.jit.si"
            android:textSize="10dp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/secondInform"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="8sp"
        android:layout_toRightOf="@id/firstInform">

        <TextView
            android:id="@+id/secondInformLesson"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:gravity="top"
            android:text='Дисциплина по выбору "Информационные технологии в прогнозировании экономических процессов,технологии в прогнозировании экономических процессов"'
            android:textSize="15dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/secondInformTeacher"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/secondInformLesson"
            android:layout_alignParentBottom="true"
            android:gravity="bottom|left"
            android:text="Пантелеев Владимир Иннокентиевич"
            android:textAlignment="gravity"
            android:textSize="12dp" />
    </RelativeLayout>

